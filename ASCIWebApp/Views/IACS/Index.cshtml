@using ASCIWebApp;
@using Microsoft.AspNetCore.Http;
@{
    ViewData["Title"] = "Home Page";
}

<header>
    <div class="content-wrapper">
        <div class="float-left">

        </div>
    </div>
</header>

<div id="body">

    <form method="post" enctype="multipart/form-data">
        <div>
            <label for="file">Choose file to upload</label><br>
            <div>Upload Xml file</div>
            <input type="file" id="file" name="filexml" accept="xml">
            <div>Upload Xlsx file</div>
            <input type="file" id="file" name="filexlsx" accept="xlsx">

        </div>
        <div>
            <button  asp-action="UploadDataFromXml" >Submit</button>
        </div>
    </form>
</div>