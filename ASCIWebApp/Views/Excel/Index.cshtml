@using ASCIWebApp;
@using Microsoft.AspNetCore.Http;
@{
   // ViewData["Title"] = "Home Page";
}

<header>
    <div class="content-wrapper">
        <div class="float-left">

        </div>
    </div>
</header>

<div id="body">

    <form method="post" enctype="multipart/form-data">
        <div>
            <label for="file">Choose file to upload</label><br>
            <div>Upload Excel file</div>
            <input type="file" id="file" name="xmlfile" accept="xlsx">
            @*<div>Upload Xlsx file</div>
                <input type="file" id="file" name="xlsx" accept="xlsx">*@

        </div>
        <div>
            <button asp-action="UploadFileExcelToServer">Submit</button>
        </div>
    </form>
    @if (TempData["Message"] != null)
    {
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
        <script type="text/javascript">
            $(document).ready(function () {
                alert("@TempData["Message"]");
            });
        </script>
    }
</div>