@using ASCIWebApp;
@using Microsoft.AspNetCore.Http;
<header>
    <div class="content-wrapper">
        <div class="float-left">

        </div>
    </div>
</header>
<div id="body">
    <form method="post" enctype="multipart/form-data">
        <div>
            <label for="file">Choose file to upload</label><br>
            <div>Upload Xml file</div>
            <input type="file" id="file" name="xml" accept="xml">
            <div>Upload Excel file</div>
            <input type="file" id="file" name="excel" accept="xlsx">
        </div>
        <div>
            <button asp-action="UploadFileToServer">Submit</button>
        </div>
    </form>
</div>